{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "RAM_Db": "Server=(localdb)\\MSSQLLocalDB;Database=RAM.Db;Trusted_Connection=True;"
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:5000"
      },
      "Https": {
        "Url": "https://0.0.0.0:5001",
        "Certificate": {
          "Path": "path/to/your/certificate.pfx",
          "Password": "your_certificate_password"
        }
      }
    }
  }
}
